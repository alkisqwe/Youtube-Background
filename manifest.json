{
  "manifest_version": 3,
  "name": "Youtube Background",
  "version": "1.0",
  "description": "Youtube Background Player For Android Chrome Version",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts":
  [
    {
      "js": ["content.js"],
      "matches":
      [
        "https://m.youtube.com/*",
        "https://music.youtube.com/*"
      ]
    }
  ]
}
